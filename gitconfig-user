[alias]
	# Push the current branch upstream to mikemcquaid using the same
	# branch name for the remote branch.
	um = !(git remote -v | grep -q MariusCiocanel || hub fork) \
	    && git push --set-upstream MariusCiocanel $(git current-branch)
